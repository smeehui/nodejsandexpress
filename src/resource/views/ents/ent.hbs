<div class="col-lg-12 row justify-content-between mt-lg-4">
    <div class="col-lg-3 bg-light text-center">
        <button class="btn btn-primary"> A btn</button>
    </div>
    {{#with ent}}
        <div class="card col-8">
            <h3 class="card-title">{{this.industry_name_ANZSIC}}</h3>
            <img
                class="card-img-top mt-4"
                src="https://d3mxt5v3yxgcsr.cloudfront.net/courses/5278/course_5278_image.png"
                alt="Card image cap"
            />
            <div class="card-body">
                <p class="card-text">{{this.variable}}</p>
                <div
                    class="row col-12 justify-content-between align-items-center"
                >
                    <a href="#" class="btn btn-primary col-5">{{this.value}}</a>
                    <a href="#" class="btn btn-primary col-5">{{this.unit}}</a>
                </div>
            </div>
        </div>
    {{/with}}
</div>